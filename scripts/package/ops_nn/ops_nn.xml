<?xml version="1.0" encoding="UTF-8"?>
<config name="ops_nn" copy_type="delivery" src_path="" dst_path="ops_nn" install_type="all" install_mod="550" install_own="$username:$usergroup">
    <package_info>
        <product_name>cann</product_name>
        <func_name>ops-nn</func_name>
        <suffix>run</suffix>
        <install_script>share/info/ops_nn/script/install.sh</install_script>
        <help>share/info/ops_nn/script/help.info</help>
        <version>package/ops_nn/version.xml</version>
        <cleanup>share/info/ops_nn/script/cleanup.sh</cleanup>
        <check_features>true</check_features>
    </package_info>
    <generate_info value="ops_nn_version.h" dst_path="share/info/ops_nn" install_path="$(TARGET_ENV)/include/version" install_mod="440" install_type="devel" generator="version_header">
       <OPS_NN_VERSION>$(ASCEND_VER)</OPS_NN_VERSION>
       <OPS_NN_TIMESTAMP>$(TIMESTAMP_NO)</OPS_NN_TIMESTAMP>
    </generate_info>
    <generate_info value="scene.info" dst_path="share/info/ops_nn" install_path="share/info/ops_nn" install_mod="440">
        <os>$(OS_NAME)</os>
        <os_version>$(OS_VER)</os_version>
        <arch>$(ARCH)</arch>
    </generate_info>
    <version_info install="true" install_path="ops_nn" install_own="$username:$usergroup" install_mod="440"/>
    <file_info value="ops_nn_script" copy_type="source" src_path="" dst_path="share/info/ops_nn" install_path="share/info/ops_nn">
        <file value="script" entity="true"/>
        <file value="version.info"/>
    </file_info>
    <!--block_info-->
    <block_info dependtree="false" dst_path="ops_nn" block_conf_path="ascend">
        <block name="EngineeringCommon"/>
        <block name="EngineeringFiles"/>
    </block_info>
    <block_info dependtree="false" dst_path="opp" block_conf_path="ascend">
        <block name="OpsNNInfo"/>
    </block_info>
    <!--dir_info-->
    <dir_info value="ops_nn" install_path="ops_nn" install_mod="550" install_type="all">
        <path value="share" install_mod="750"/>
        <path value="share/info" install_mod="750"/>
        <path value="share/info/ops_nn" install_mod="550"/>
        <path value="share/info/ops_nn/script" install_mod="550"/>
    </dir_info>
</config>
